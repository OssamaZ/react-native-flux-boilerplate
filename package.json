{
  "name": "ReactNativeFluxBoilerplate",
  "version": "0.0.1",
  "description": "React native boilerplate with Flux, React native webpack and hot reload",
  "keywords": [
    "react native",
    "flux",
    "webpack",
    "hot reload"
  ],
  "homepage": "https://github.com/OssamaZ/react-native-flux-boilerplate",
  "private": true,
  "scripts": {
