  "dependencies": {
    "react-native": "^0.11.0"
  },
  "devDependencies": {
    "babel-core": "^5.8.24",
    "babel-loader": "^5.3.2",
    "events": "^1.0.2",
    "flux": "^2.1.1",
    "keymirror": "^0.1.1",
    "lodash": "^3.10.1",
    "react": "^0.13.3",
    "react-hot-loader": "^1.3.0",
    "react-native-webpack-server": "^0.4.0",
    "webpack": "^1.12.2",
    "webpack-dev-server": "^1.11.0"
  }
}
